        <%= previous_month_link(:diary_cal,'Prev',style:'color:red;',class:'nav'){true} %>
    </div>
    <div class="col-lg-1 text-center">
        <%= next_month_link(:diary_cal,'Next',style:'color:red;',class:'nav'){true} %>
